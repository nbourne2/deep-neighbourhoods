<graph id="Graph">
  <version>1.0</version>
  <node id="Read">
    <operator>Read</operator>
    <sources/>
    <parameters class="com.bc.ceres.binding.dom.XppDomElement">
      <file>${sourceProduct}</file>
    </parameters>
  </node>
  <node id="Resample">
    <operator>Resample</operator>
    <sources>
      <sourceProduct refid="Read"/>
    </sources>
    <parameters class="com.bc.ceres.binding.dom.XppDomElement">
      <referenceBand>B2</referenceBand>
      <targetWidth/>
      <targetHeight/>
      <targetResolution/>
      <upsampling>Nearest</upsampling>
      <downsampling>First</downsampling>
      <flagDownsampling>First</flagDownsampling>
      <resampleOnPyramidLevels>true</resampleOnPyramidLevels>
    </parameters>
  </node>
  <node id="GLCM">
    <operator>GLCM</operator>
    <sources>
      <sourceProduct refid="Resample"/>
    </sources>
    <parameters class="com.bc.ceres.binding.dom.XppDomElement">
      <sourceBands>B3,B5,B6</sourceBands>
      <windowSizeStr>5x5</windowSizeStr>
      <angleStr>ALL</angleStr>
      <quantizerStr>Probabilistic Quantizer</quantizerStr>
      <quantizationLevelsStr>32</quantizationLevelsStr>
      <displacement>1</displacement>
      <outputContrast>false</outputContrast>
      <outputDissimilarity>true</outputDissimilarity>
      <outputHomogeneity>true</outputHomogeneity>
      <outputASM>false</outputASM>
      <outputEnergy>false</outputEnergy>
      <outputMAX>false</outputMAX>
      <outputEntropy>false</outputEntropy>
      <outputMean>true</outputMean>
      <outputVariance>true</outputVariance>
      <outputCorrelation>false</outputCorrelation>
    </parameters>
  </node>
  <node id="PCA">
    <operator>PCA</operator>
    <sources>
      <sourceProduct refid="GLCM"/>
    </sources>
    <parameters class="com.bc.ceres.binding.dom.XppDomElement">
      <sourceBandNames/>
      <componentCount>3</componentCount>
      <roiMaskName/>
      <removeNonRoiPixels>false</removeNonRoiPixels>
    </parameters>
  </node>
  <node id="Write">
    <operator>Write</operator>
    <sources>
      <sourceProduct refid="PCA"/>
    </sources>
    <parameters class="com.bc.ceres.binding.dom.XppDomElement">
      <file>${targetProduct}</file>
      <formatName>BEAM-DIMAP</formatName>
    </parameters>
  </node>
  <applicationData id="Presentation">
    <Description/>
    <node id="Read">
            <displayPosition x="8.0" y="19.0"/>
    </node>
    <node id="Resample">
      <displayPosition x="95.0" y="74.0"/>
    </node>
    <node id="GLCM">
      <displayPosition x="198.0" y="126.0"/>
    </node>
    <node id="PCA">
      <displayPosition x="297.0" y="170.0"/>
    </node>
    <node id="Write">
      <displayPosition x="391.0" y="215.0"/>
    </node>
  </applicationData>
</graph>
